{
  "name": "nativescript-collapsing-header",
  "version": "2.0.0",
  "description": "NativeScript Collapsing Header Plugin.",
  "main": "nativescript-collapsing-header.js",
  "nativescript": {
    "platforms": {
      "android": "1.7.0",
      "ios": "1.7.0"
    },
    "tns-ios": {
      "version": "1.7.0"
    }
  },
  "scripts": {
    "build": "tsc",
    "demo.ios": "npm run preparedemo &&  cd demo &&  tns run ios",
    "demo.android": "npm run preparedemo &&  cd demo &&  tns run android",
    "preparedemo": "npm run build &&  cd demo &&  tns plugin remove nativescript-collapsing-header &&  tns plugin add .. &&  tns install",
    "setup": "cd demo &&  npm install &&  cd .. &&  npm run build &&  cd demo &&  tns plugin add .. &&  cd .."
  },
